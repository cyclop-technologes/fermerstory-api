{
  "connection": "default",
  "collectionName": "order",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "required": true,
      "type": "string"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "address": {
      "type": "string"
    },
    "date": {
      "type": "datetime"
    },
    "paymantOnline": {
      "type": "boolean"
    },
    "paid": {
      "type": "boolean"
    },
    "amount": {
      "type": "integer",
      "required": true
    },
    "delivery": {
      "type": "boolean"
    },
    "store": {
      "type": "string"
    },
    "products": {
      "component": "cart.product",
      "repeatable": true,
      "type": "component"
    }
  }
}
