{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "comments",
  "info": {
    "name": "Comment"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "comments"
    },
    "rating": {
      "type": "integer"
    },
    "text": {
      "type": "text"
    },
    "product": {
      "model": "product",
      "via": "comment"
    }
  }
}
