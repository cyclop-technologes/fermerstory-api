{
  "connection": "default",
  "collectionName": "product",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "default": "наборы",
      "enum": [
        "наборы",
        "овощи",
        "фрукты",
        "экзотика",
        "молочка и яйца",
        "сыры",
        "мясо и колбасы",
        "хлеб",
        "бакалея",
        "полуфабрикаты"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "photo": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "weight": {
      "type": "integer"
    },
    "calories": {
      "type": "integer"
    },
    "fat": {
      "type": "decimal"
    },
    "carbohydrate": {
      "type": "decimal"
    },
    "protein": {
      "type": "decimal"
    },
    "stocked": {
      "default": true,
      "type": "boolean"
    },
    "farmer": {
      "model": "farmer",
      "via": "products"
    }
  }
}
